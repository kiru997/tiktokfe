{
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": [
        "airbnb",
        "prettier",
        "plugin:react/recommended",
        "plugin:prettier/recommended",
        "prettier/react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true,
            "jsx": false
        }
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
         "no-console": 1
      },
      "globals": {
        "axios": true,
        "PropTypes": true
    }
}